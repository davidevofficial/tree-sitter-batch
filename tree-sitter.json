{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "batch",
      "camelcase": "Batch",
      "title": "Batch",
      "scope": "source.dosbatch",
      "file-types": [
        "dosbatch",
        "bat",
        "batch"
      ],
      "injection-regex": "^batch$",
      "class-name": "TreeSitterBatch"
    }
  ],
  "metadata": {
    "version": "0.0.1",
    "license": "MIT",
    "description": "Batch tree sitter",
    "authors": [
      {
        "name": "Davidevofficial",
        "email": "davidevufficial@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/davidevofficial/tree-sitter-batch",
      "funding": "https://github.com/davidevofficial/tree-sitter-batch"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": false,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
